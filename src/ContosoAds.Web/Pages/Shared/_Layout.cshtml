<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

<footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2022 - Contoso Ads - <a asp-area="" asp-page="/Privacy">Privacy</a>
    </div>
</footer>

